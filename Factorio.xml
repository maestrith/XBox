				<Button Button="Up"></Button></KeyPress>
			<KeyPress Key="A">
				<Button Button="Left"></Button></KeyPress>
			<KeyPress Key="S">
				<Button Button="Down"></Button></KeyPress>
			<KeyPress Key="D">
				<Button Button="Right"></Button></KeyPress></Group>
		<KeyPress Key="LButton" expand="1">
			<Trigger Trigger="RTrigger">
				<Dead Dead="4"></Dead></Trigger>
			<Hold Hold="Control"></Hold></KeyPress></Alt>
	<Alt name="RightShoulder" expand="1">
