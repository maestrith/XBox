				<Hold Hold="Shift"></Hold>
				<Axis Axis="LThumbY">
					<Direction Direction="Down"></Direction></Axis></KeyPress>
			<KeyPress Key="3">
				<Hold Hold="Shift"></Hold>
				<Axis Axis="LThumbX">
					<Direction Direction="Right"></Direction></Axis></KeyPress>
			<KeyPress Key="4">
				<Hold Hold="Shift"></Hold>
				<Axis Axis="LThumbY">
					<Direction Direction="Up"></Direction></Axis></KeyPress>
			<KeyPress Key="5">
				<Hold Hold="Shift"></Hold>
				<Button Button="LeftThumb"></Button></KeyPress></Group>
		<KeyPress Key="WheelUp" expand="1">
			<Button Button="Y"></Button></KeyPress>
		<KeyPress Key="WheelDown" expand="1">
			<Button Button="B"></Button></KeyPress></Alt></game>
