<?xml version="1.0"?>
<game name="RimWorld" expand="1">
	<DirectInput name="Direct Input" on="1"></DirectInput>
	<Alt main="1" name="Main" expand="1">
		<Group name="Movement" expand="1">
			<KeyPress Key="W" expand="1">
				<Button Button="Up"></Button></KeyPress>
			<KeyPress Key="A" expand="1">
				<Button Button="Left"></Button></KeyPress>
			<KeyPress Key="S" expand="1">
				<Button Button="Down"></Button></KeyPress>
			<KeyPress Key="D" expand="1">
				<Button Button="Right"></Button></KeyPress>
			<KeyPress Key="Escape" expand="1">
				<Button Button="Start"></Button></KeyPress>
			<KeyPress Key="Tab" expand="1">
				<Button Button="Back"></Button></KeyPress>
			<KeyPress Key="Space" expand="1">
				<Button Button="Y"></Button></KeyPress>
			<Mouse expand="1">
				<Read Read="LThumb" expand="1">
					<Dead Dead="3"></Dead>
					<Speed Speed="10"></Speed></Read></Mouse></Group>
		<Mouse expand="1">
			<Read Read="RThumb" expand="1">
				<Dead Dead="3"></Dead>
				<Speed Speed="4"></Speed></Read></Mouse>
		<KeyPress Key="LButton" expand="1">
			<Button Button="X"></Button></KeyPress>
		<KeyPress Key="RButton" expand="1">
			<Button Button="A"></Button></KeyPress></Alt></game>
